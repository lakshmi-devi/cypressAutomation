{
  "name": "automation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "open": "cypress open",
    "test-electron": "cypress run --spec \"cypress\\e2e\\Tc01-Testscript.spec.cy.js\"",
    "smoke-test": "cypress run --spec \"cypress\\\\e2e\\\\smoketest\\\\Tc01-smokespec.cy.js\"",
    "test-electron-headed": "cypress run --headed",
    "test-chrome": "cypress run --browser chrome",
    "test-chrome-headed": "cypress run --browser chrome --headed",
    "test-dashboard": "npx cypress run --record --key fc47dea7-4a3d-42f6-8908-82eb8e1d3ae4",
    "scripts": "cypress run --spec \"cypress\\e2e\\smoketest\\*.js\"",
    "clean:reports": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports",
    "pretest": "npm run clean:reports",
    "combine-reports": "mochawesome-merge  ./cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-reports": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-reports",
    "test": "npm run scripts || npm run posttest"
  },
  "author": "db",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "-": "^0.0.1",
    "cypress": "^13.15.2",
    "cypress-multi-reporters": "^2.0.4",
    "cypress-xpath": "^2.0.1",
    "merge": "^2.1.1",
    "mocha": "^11.0.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "report-generator": "^1.0.5",
    "xlsx": "^0.18.5"
  }
}
